programa {
  funcao inteiro somador(inteiro valor1, inteiro valor2){
    retorne valor1 + valor2
  }
  funcao inicio() {
    inteiro valor1, valor2, valor
    escreva("Digite o primeiro valor: ")
    leia(valor1)
    escreva("Digite o segundo valor: ")
    leia(valor2)

    valor = somador(valor1,valor2)

    escreva(valor)
    

  }
}
